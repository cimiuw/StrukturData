import java.util.Arrays;
public class arraywana {
   
    public static void main(String[] args) {
        // Membuat array 1 dimensi bertipe data primitif
        int[] angka = {1, 2, 3, 4, 5};
        
        // Membuat array 1 dimensi bertipe data non primitif
        String[] nama = {"wana", "wini", "wonu"};
        
        // Membuat array 2 dimensi bertipe data primitif
        char[][] abjad = {
            {'a', 'b', 'c'},
            {'d', 'e', 'f'},
            {'g', 'h', 'i'}
        };
        // Mencetak array 1 dimensi bertipe data primitif
        System.out.println(Arrays.toString(angka));
        // Mencetak array 1 dimensi bertipe data non primitif
        System.out.println(Arrays.toString(nama));
        // Mencetak array 2 dimensi bertipe data primitif
        System.out.println(Arrays.deepToString(abjad));
     
}
}
